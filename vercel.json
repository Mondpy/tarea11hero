{
    "builds":[
        {
            "src":"python_heroku_task/wsgi.py",
            "use":"@vercel/python",
            "confi": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10.12",
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python_heroku_task/wsgi.py"
        }
    ]
}